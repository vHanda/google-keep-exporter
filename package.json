{
  "name": "google-keep-exporter",
  "version": "1.2.0",
  "description": "This is a tool which converts the Google Keep archive to markdown documents with YAML front matter. The Google Keep archive can be obtained from the Google Takeout page.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "pkg": "pkg -o google-keep-exporter --targets 'node10-linux,node10-macos,node10-win' dist/main.js",
    "test": "jest src/**"
  },
  "author": "Vishesh Handa <me@vhanda.in>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vhanda/google-keep-exporter.git"
  },
  "dependencies": {
    "cheerio": "0.22.0",
    "chrono-node": "^2.2.4",
    "extract-zip": "^1.6.7",
    "js-yaml": "^3.12.0",
    "tmp": "0.0.33",
    "turndown": "^5.0.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.9",
    "@types/extract-zip": "^1.6.2",
    "@types/jest": "^23.3.2",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.9.4",
    "@types/tmp": "0.0.33",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "pkg": "^4.3.4",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
